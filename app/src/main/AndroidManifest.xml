<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="qidizi.openAs" >

    <application
        android:allowBackup="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
		android:resizeableActivity = "true">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
			<!--
			不显示图标
            <intent-filter>
			<action android:name="android.intent.action.MAIN" />

			<category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
			-->

			<intent-filter>
				<action android:name="android.intent.action.SEARCH"/>
				<action android:name="android.intent.action.DEFINE"/>
				<action android:name="android.intent.action.CREATE_DOCUMENT"/>
				<action android:name="android.intent.action.APP_ERROR"/>
				<action android:name="android.intent.action.OPEN_DOCUMENT"/>
				<action android:name="android.intent.action.TRANSLATE"/>
				<action android:name="android.intent.action.QUICK_VIEW"/>
				<action android:name="android.intent.action.SHOW_APP_INFO"/>
				<action android:name="android.intent.action.VIEW"/>
				<action android:name="android.intent.action.VIDEO_PLAYER"/>
				<action android:name="android.intent.action.MUSIC_PLAYER"/>
				<action android:name="android.intent.action.RUN"/>
				<action android:name="android.intent.action.EDIT"/>
				<action android:name="android.intent.action.BUG_REPORT"/>
				<action android:name="android.intent.action.WEB_SEARCH"/>
				<action android:name="android.intent.action.SENDTO"/>
					<!-- 分享列表中-->
				<action android:name="android.intent.action.SEND"/>
				<action android:name="android.intent.action.GET_CONTENT"/>
				<action android:name="android.intent.action.CALL"/>
				<action android:name="android.intent.action.ADD_SHORTCUT"/>


				<category android:name="android.intent.category.BROWSABLE"/>
				<category android:name="android.intent.category.DEFAULT"/>
				<category android:name="android.intent.category.APP_MARKET"/>
				<category android:name="android.intent.category.APP_MUSIC"/>
				
				<category android:name="android.intent.category.SELECTED_ALTERNATIVE"/>
				<category android:name="android.intent.category.OPENABLE"/>
				<data android:mimeType="*/*"/>
				
			
			</intent-filter>
			

			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
				<category android:name="android.intent.category.DEFAULT" />
				<category android:name="android.intent.category.BROWSABLE" />
				<data android:scheme="http" />
				<data android:mimeType="*/*"/>
			</intent-filter>


			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
				<category android:name="android.intent.category.DEFAULT" />
				<category android:name="android.intent.category.BROWSABLE" />
				<data android:scheme="*" />
				<data android:mimeType="*/*"/>
			</intent-filter>

			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
				<category android:name="android.intent.category.DEFAULT" />
				<category android:name="android.intent.category.BROWSABLE" />
				<data android:scheme="*" />
				<data android:mimeType="video/*"/>
				<data android:scheme="file"/>
				<data android:scheme="content"/>
				<data android:host="*"/>
				<data android:scheme="http"/>
			</intent-filter>
			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
				<category android:name="android.intent.category.DEFAULT" />
				<category android:name="android.intent.category.BROWSABLE" />
				<data android:scheme="http" />
				<data android:mimeType="video/*" />
			</intent-filter>
			
			</activity>
			
		</application>
	</manifest>
